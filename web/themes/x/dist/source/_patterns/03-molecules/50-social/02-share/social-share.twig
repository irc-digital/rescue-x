{#
/**
 * Available variables:
 *     social_share_sites           - Used to generate social share icon
 *
 * Available blocks:
 * - none
 */
#}
<div class="rplm-social-share">
  {% for social_share_site in social_share_sites %}
    <a class="rplm-social-share__link" href="#">
      {% include '@atoms/15-images/20-icon/icon.twig' with {
        icon_name: social_share_site.icon_name,
      } only %}
    </a>
  {% endfor %}
</div>