<div class="rpll-global-navigation-layout">
  <div class="rpll-global-navigation-layout__top-wrapper">
    <div class="rpll-global-navigation-layout__desktop-top-wrapper">
      <div class="rpll-global-navigation-layout__desktop-crisis-watch-wrapper">
        {{ global_navigation_layout_crisis_watch }}
      </div>
      <div class="rpll-global-navigation-layout__desktop-secondary-menu-wrapper">
        {{ global_navigation_layout_secondary_menu }}
      </div>
    </div>
    <div class="rpll-global-navigation-layout__mobile-top-wrapper">
      <div class="rpll-global-navigation-layout__mobile-branding-wrapper">
        {{ global_navigation_layout_mobile_branding }}
      </div>
      <div class="rpll-global-navigation-layout__mobile-call-to-action-menu-wrapper">
        {{ global_navigation_layout_mobile_call_to_action_menu }}
      </div>
    </div>
  </div>
  <div class="rpll-global-navigation-layout__bottom-wrapper">
    <div class="rpll-global-navigation-layout__main-menu-wrapper">
      {{ global_navigation_layout_main_menu }}
    </div>
    <div class="rpll-global-navigation-layout__desktop-trailing-wrapper">
      {{ global_navigation_layout_desktop_trailing }}
    </div>
    <div class="rpll-global-navigation-layout__mobile-secondary-wrapper">
      {{ global_navigation_layout_mobile_secondary_menu }}
    </div>
  </div>
</div>
