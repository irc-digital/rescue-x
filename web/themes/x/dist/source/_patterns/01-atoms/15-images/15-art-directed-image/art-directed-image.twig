<picture class='rpla-art-directed-image'>
  {% for art_directed_image_source in art_directed_image_sources %}
  <source srcset="{% for source_set in art_directed_image_source.source_set %}{{ source_set }},{% endfor %}"{% if art_directed_image_source.media %} media="{{ art_directed_image_source.media }}"{% endif %}>
  {% endfor%}
  <img srcset="{{ art_directed_image_fallback }}"{% if art_directed_image_alt %} alt="{{ art_directed_image_alt }}"{% endif %}/>
</picture>




