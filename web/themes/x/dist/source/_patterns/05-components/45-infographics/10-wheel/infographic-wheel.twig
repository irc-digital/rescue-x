<div class="rplc-infographic-wheel">
  {% for infographic_wheel_item in infographic_wheel_content %}
    <div class="rplc-infographic-wheel__item-wrapper">
      <div class="rplc-infographic-wheel__title">{{ infographic_wheel_item.title }}</div>
      <div class="rplc-infographic-wheel__icon">
        {% include '@atoms/05-icon/icon.twig' with {
          icon_name: infographic_wheel_item.icon,
        } only %}
      </div>
    </div>
  {% endfor %}
</div>