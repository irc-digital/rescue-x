<div class="rplc-secondary-menu">
  <ul class="rplc-secondary-menu__menu">
    {% for secondary_menu_menu_item in secondary_menu_menu_items %}
      <li class="rplc-secondary-menu__menu-item">
        {% include '@atoms/12-link-icon/link-icon.twig' with {
          link_icon_url: secondary_menu_menu_item.url,
          link_icon_text: secondary_menu_menu_item.title,
          link_icon_name: secondary_menu_menu_item.icon,
          link_icon_modifiers: ['type-five', 'weight-normal'],
        } only %}
      </li>
    {% endfor %}
    <li class="rplc-secondary-menu__menu-item rplc-secondary-menu__menu-item--social">
      {% include '@molecules/50-social-share/social-share.twig' with {
        social_share_sites: secondary_menu_social_share_sites,
        social_share_modifiers: ['subtype-inverse'],
      } only %}
    </li>
  </ul>
</div>