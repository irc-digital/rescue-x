{% if mobile_call_to_action_menu_items is not empty %}
<ul class="rplc-mobile-call-to-action-menu">
  {% for mobile_call_to_action_menu_item in mobile_call_to_action_menu_items %}
    <li class="rplc-mobile-call-to-action-menu__item">
      {% include '@atoms/10-link/link.twig' with {
        link_text: mobile_call_to_action_menu_item.title,
        link_url:  mobile_call_to_action_menu_item.url,
        link_modifiers: ['type-two', 'subtype-black', 'subtype-no-hover-effect'],
      } only %}
    </li>
  {% endfor %}
</ul>
{% endif %}
